while True:
    # Ask the user to enter a year
    year = int(input("Welcome to check leap year! Enter a year (or enter 'quit' to exit): "))
    
    # Check if the user wants to quit
    if year == 'quit':
        break
    
    # Check if the year is a leap year and print the result
    if year % 4 == 0:
        if year % 100 == 0:
            if year % 400 == 0:
                print("Leap year")
            else:
                print("Not Leap year")
        else:
            print("Leap year")
    else:
        print("Not Leap year")
